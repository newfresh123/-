.iot-index-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.iot-index-swiper swiper image {
  width: 100%;
}
.iot-index-cate {
  display: flex;
}
.iot-index-cate navigator {
  flex: 1;
  padding: 20rpx;
}
.iot-index-cate navigator image {
  width: 100%;
}
.iot-index-floor .iot-floor-group .iot-floor-title {
  padding: 10rpx 0;
}
.iot-index-floor .iot-floor-group .iot-floor-title image {
  width: 100%;
}
.iot-index-floor .iot-floor-group .iot-floor-list {
  overflow: hidden;
}
.iot-index-floor .iot-floor-group .iot-floor-list navigator {
  float: left;
  width: 32%;
}
.iot-index-floor .iot-floor-group .iot-floor-list navigator:nth-last-child(-n+4) {
  height:  27.72711207vw;
  border-left: 10rpx solid white;
}
.iot-index-floor .iot-floor-group .iot-floor-list navigator:nth-child(2),
.iot-index-floor .iot-floor-group .iot-floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.iot-index-floor .iot-floor-group .iot-floor-list navigator image {
  width: 100%;
  height: 100%;
}
